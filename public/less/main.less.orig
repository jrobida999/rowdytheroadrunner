h1 {
  color: @orange;
  font-size: 32px;
  font-weight: 900; 
  text-transform: uppercase;
}

h2 {
  color: @carrot;
  font-size: 26px;
  font-weight: 700;
  margin-bottom: 2px;
  text-transform: uppercase;
}

h3 {
  color: @pomegranate;
  font-size: 24px;
  font-weight: 700;
  margin-bottom: 4px;
  margin-top: 2px; }

h4 { 
  color: @peterriver;
  font-size: 18px;
  font-weight: 500;
  margin-top: 4px; }

h5 {
  color: @midnightblue;
  font-size: 16px;
  font-weight: 500;}

h6 {
  color: @alizarin;
  font-size: 13px;
  font-weight: 500;}


body {
  margin: 0;
  font-family: cabin, source-sans-pro, muli, josefin-sans, open-sans, sans-serif;
  font-size: 14px;
  line-height: 20px;
  color: #333333;
  background-color: @white;
}

a {
  color: @contentlink;
  text-decoration: none;
}

a:hover,
a:focus {
  color: @contenthlink;
  text-decoration: underline;
}





